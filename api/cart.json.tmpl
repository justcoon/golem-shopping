{
  "id": "cart",
  "version": "0.0.1",
  "routes": [
    {
      "method": "Get",
      "path": "/v1/cart/{user-id}",
      "binding": {
        "componentId": {
          "componentId": "$CART_COMPONENT_ID",
          "version": $CART_COMPONENT_VERSION
        },
        "workerName": "let id: string = request.path.user-id;\nid",
        "idempotencyKey": null,
        "response": "let result = golem:cart-exports/api.{get}();\nmatch result {  some(x) => {status: 200u64, body: x}, none => {status: 404u64, body: {message: \"not found\"}} } ",
        "idempotencyKeyInput": null
      }
    },
    {
      "method": "Put",
      "path": "/v1/cart/{user-id}/items/{product-id}",
      "binding": {
        "componentId": {
          "componentId": "$CART_COMPONENT_ID",
          "version": $CART_COMPONENT_VERSION
        },
        "workerName": "let id: string = request.path.user-id;\nid",
        "idempotencyKey": null,
        "response": "let result = golem:cart-exports/api.{add-item}(request.path.product-id, request.body.quantity);\nmatch result {  ok(_) => {status: 200u64, body: {message: \"item added\"}}, err(x) => {status: 400u64, body: {error: x}} } ",
        "idempotencyKeyInput": null
      }
    },
    {
      "method": "Post",
      "path": "/v1/cart/{user-id}/checkout",
      "binding": {
        "componentId": {
          "componentId": "$CART_COMPONENT_ID",
          "version": $CART_COMPONENT_VERSION
        },
        "workerName": "let id: string = request.path.user-id;\nid",
        "idempotencyKey": null,
        "response": "let result = golem:cart-exports/api.{checkout}();\nmatch result {  ok(x) => {status: 200u64, body: x}, err(x) => {status: 400u64, body: {error: x}} } ",
        "idempotencyKeyInput": null
      }
    },
    {
      "method": "Put",
      "path": "/v1/cart/{user-id}/email",
      "binding": {
        "componentId": {
          "componentId": "$CART_COMPONENT_ID",
          "version": $CART_COMPONENT_VERSION
        },
        "workerName": "let id: string = request.path.user-id;\nid",
        "idempotencyKey": null,
        "response": "let email: string = request.body.email;\nlet result = golem:cart-exports/api.{update-email}(email);\nmatch result {  ok(_) => {status: 200u64, body: {message: \"email set\"}}, err(x) => {status: 400u64, body: {error: x}} } ",
        "idempotencyKeyInput": null
      }
    },
    {
      "method": "Put",
      "path": "/v1/cart/{user-id}/billing-address",
      "binding": {
        "componentId": {
          "componentId": "$CART_COMPONENT_ID",
          "version": $CART_COMPONENT_VERSION
        },
        "workerName": "let id: string = request.path.user-id;\nid",
        "idempotencyKey": null,
        "response": "let result = golem:cart-exports/api.{update-billing-address}(request.body);\nmatch result {  ok(_) => {status: 200u64, body: {message: \"address set\"}}, err(x) => {status: 400u64, body: {error: x}} } ",
        "idempotencyKeyInput": null
      }
    },
    {
      "method": "Put",
      "path": "/v1/cart/{user-id}/shipping-address",
      "binding": {
        "componentId": {
          "componentId": "$CART_COMPONENT_ID",
          "version": $CART_COMPONENT_VERSION
        },
        "workerName": "let id: string = request.path.user-id;\nid",
        "idempotencyKey": null,
        "response": "let result = golem:cart-exports/api.{update-shipping-address}(request.body);\nmatch result {  ok(_) => {status: 200u64, body: {message: \"address set\"}}, err(x) => {status: 400u64, body: {error: x}} } ",
        "idempotencyKeyInput": null
      }
    },
    {
      "method": "Delete",
      "path": "/v1/cart/{user-id}/items/{product-id}",
      "binding": {
        "componentId": {
          "componentId": "$CART_COMPONENT_ID",
          "version": $CART_COMPONENT_VERSION
        },
        "workerName": "let id: string = request.path.user-id;\nid",
        "idempotencyKey": null,
        "response": "let result = golem:cart-exports/api.{remove-item}(request.path.product-id);\nmatch result {  ok(_) => {status: 200u64, body: {message: \"item removed\"}}, err(x) => {status: 400u64, body: {error: x}} } ",
        "idempotencyKeyInput": null
      }
    }
  ],
  "draft": true
}