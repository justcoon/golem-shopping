package golem:product-search-exports;

interface api {
  use wasi:clocks/wall-clock@0.2.0.{ datetime };
  record product {
    product-id: string,
    name: string,
    brand: string,
    description: string,
    tags: list<string>,
    created-at: datetime,
    updated-at: datetime,
  }
  record product-filter {
    name: option<string>,
    brand: option<string>,
  }
  find: func(filter: product-filter) -> list<product>;
}

interface product-search-inline-functions {}
