package golem:product-search-exports;

interface api {
  use wasi:clocks/wall-clock@0.2.0.{ datetime };
  record product {
    product-id: string,
    name: string,
    brand: string,
    description: string,
    tags: list<string>,
    created-at: datetime,
    updated-at: datetime,
  }
  search: func(query: string) -> result<list<product>, string>;
}

interface product-search-inline-functions {}
